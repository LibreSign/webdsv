{
    "name": "libresign/webdsv",
    "description": "Digital Signature Validation - Web version",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "https://github.com/libresign/webdsv/",
    "license": "AGPL",
    "require": {
        "ext-json": "*",
        "monolog/monolog": "^3.2",
        "ncjoes/poppler-php": "dev-feature/bump-versions",
        "php-di/php-di": "^6.4",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.11",
        "slim/twig-view": "^3.3",
        "twig/twig": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/LibreSign/poppler-php"
        }
    ]
}
